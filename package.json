{
  "name": "react-init",
  "version": "1.0.0",
  "description": "setup react app from scratch",
  "scripts": {
    "dev": "webpack server --mode=development --env TARGET=development",
    "build": "webpack --mode=production --progress",
    "build:production": "webpack --mode=production --progress --env TARGET=production",
    "prettier": "prettier 'src/**/*.{ts,tsx}'",
    "prettier:check": "npm run prettier -- --check",
    "prettier:format": "npm run prettier -- --write",
    "lint": "eslint src --ext .js,.ts,.tsx,.jsx",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaibhavmande/react-init.git"
  },
  "author": "vaibhavmande",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vaibhavmande/react-init/issues"
  },
  "homepage": "https://github.com/vaibhavmande/react-init#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^30.0.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "2.8.1",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "core-js": "^3.27.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
