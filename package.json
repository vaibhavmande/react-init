{
  "name": "react-init",
  "version": "1.0.0",
  "description": "setup react app from scratch",
  "scripts": {
    "dev:live": "webpack server --mode=development --env TARGET_ENV=development SERVICE_URL=",
    "build": "webpack --mode=production",
    "build:production": "webpack --mode=production --progress",
    "build:dev": "webpack --mode=development --progress",
    "type-check": "echo \"type-checking...\" && tsc --noEmit --skipLibCheck",
    "test": "jest",
    "prettier": "prettier '{src,e2e}/**/*.{ts,tsx}'",
    "prettier:check": "npm run prettier -- --check",
    "prettier:format": "npm run prettier -- --write",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src --ext .js,.ts,.tsx,.jsx",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint -- --max-warnings=2"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaibhavmande/react-init.git"
  },
  "author": "vaibhavmande",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^30.0.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.1",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "core-js": "^3.27.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.7.0"
  }
}
